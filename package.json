{
  "name": "linting-with-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "gsap": "^3.11.3",
    "lottie-vuejs": "^0.4.0",
    "lottie-web-vue": "^2.0.6",
    "pug": "^3.0.2",
    "pug-plain-loader": "^1.1.0",
    "sass": "^1.32.11",
    "sass-loader": "^12.1.0",
    "swiper": "^8.4.4",
    "three": "^0.145.0",
    "three-obj-exporter": "^0.0.1",
    "three-trackballcontrols": "^0.9.0",
    "vue": "^3.2.13",
    "vue-3d-model": "^1.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@volar-plugins/vetur": "latest",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "no-tabs": [
        "error",
        {
          "allowIndentationTabs": true
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
